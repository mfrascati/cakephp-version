{
    "name": "mfrascati/cakephp-version",
    "description": "Versions plugin for CakePHP",
    "type": "cakephp-plugin",
    "require": {
        "php": ">=5.4.16",
        "cakephp/orm": "~3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "Entheos\\Versions\\": "src",
            "Entheos\\Versions\\Test\\Fixture\\": "tests\\Fixture"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Entheos\\Versions\\Test\\": "tests"
        }
    }
}
